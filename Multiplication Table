package com.company;

public class Main {

    public static void main(String[] args) {

        int a= 1;
        int b= 2;
        int c =3;
        int d = 4;
        System.out.println (" " + "  1" + "  2" + "  3" + "  4");

        for (int j = 1; j<= 4; j++)
        {
            System.out.print(j + "  ");
            for (int i = 1; i <= 4; i++)
            {
                System.out.print(i * j + "  ");
            }
            System.out.println();
        }
    }
}

